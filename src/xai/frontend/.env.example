# XAI Trading Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:8001
REACT_APP_WS_URL=ws://localhost:8001/ws

# Authentication
REACT_APP_AUTH_ENABLED=true
REACT_APP_AUTH_PROVIDER=local

# Features
REACT_APP_ENABLE_VOICE=true
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_EXPORT=true

# Analytics
REACT_APP_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=

# Development
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info

# Performance
REACT_APP_CACHE_SIZE=1000
REACT_APP_WS_RECONNECT_ATTEMPTS=5
REACT_APP_API_TIMEOUT=30000

# UI Configuration
REACT_APP_THEME=auto
REACT_APP_DEFAULT_TIMEFRAME=1d
REACT_APP_DEFAULT_SYMBOLS=NQ,ES

# Security
REACT_APP_CSP_NONCE=
REACT_APP_ENABLE_CSP=true

# External Services
REACT_APP_CHART_PROVIDER=chartjs
REACT_APP_VOICE_PROVIDER=browser